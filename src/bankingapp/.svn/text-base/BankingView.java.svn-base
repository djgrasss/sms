    public void doAction(int action){
        switch(action){
            case ACTION_FDDEPOSIT:
                fd_new();
                break;
            case ACTION_FDWITHDRAW:
                fd_close();
                break;
            case ACTION_SBDEPOSIT:
                sb_deposit();
                break; 
            case ACTION_SBWITHDRAW:
                sb_withdraw();
                break;  
            case ACTION_MDS:
                mds_payment();
                break;    
            case ACTION_LOAN:
                loan_repayment();
                break;        
        }
    }

