    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        String sql = "select members.id,members.username from accounts inner join schemes on accounts.scheme_id =schemes.id  inner join members on accounts.member_id = members.id inner join transactions on accounts.id = transactions.account_id  where accounts.closed=0 and (schemes.scheme='Long Term Loan' or schemes.scheme='Short Term Loan' or schemes.scheme='MDS Loan' or schemes.scheme='HP Loan' or schemes.scheme='Pay Security Loan' or schemes.scheme='mds' ) and transactions.`timestamp`<DATE_SUB(NOW(),INTERVAL 1 MONTH) group by accounts.id";
        Query q = BankingPUEntityManager.createNativeQuery(sql);
        List l = q.getResultList();
        for (Iterator it = l.iterator(); it.hasNext();) {
            Object object = it.next();
            Vector v = (Vector)object;
            Integer uid = (Integer) v.get(0);
            Query qqy = BankingPUEntityManager.createNamedQuery("MemberDetails1.findByMemberId");
            qqy.setParameter("memberId",uid);
            MemberDetails1 md = (MemberDetails1) qqy.getSingleResult();
            addressPane.setText(addressPane.getText()+md.getName()+"\n"+md.getAddress()+"\n\n\n");
            jDialog1.setVisible(true);
        }

    }//GEN-LAST:event_jButton1ActionPerformed

