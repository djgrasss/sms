    private void populateTable(){
        //String dd = "2010-07-16";
        String[] columns = {"Member Number","Username","A/C No","Scheme","Last Account Activity"};
        String sql = "select members.number,members.username,accounts.account_number,schemes.scheme,accounts.id,max(transactions.timestamp) from accounts inner join schemes on accounts.scheme_id =schemes.id  inner join members on accounts.member_id = members.id inner join transactions on accounts.id = transactions.account_id  where accounts.closed=0 and (schemes.scheme='Long Term Loan' or schemes.scheme='Short Term Loan' or schemes.scheme='MDS Loan' or schemes.scheme='HP Loan' or schemes.scheme='Pay Security Loan' or schemes.scheme='mds' ) and transactions.`timestamp`<DATE_SUB(NOW(),INTERVAL 1 MONTH) group by accounts.id";
        Query q = BankingPUEntityManager.createNativeQuery(sql);
        List l = q.getResultList();
        String[][] data = new String[l.size()][5];
        int i=0;
        for (Iterator it = l.iterator(); it.hasNext();) {
            Object object = it.next();
            Vector v = (Vector)object;
            data[i][0]= v.get(0)==null ? " ":v.get(0).toString();
            data[i][1]= v.get(1)==null ? " ":v.get(1).toString();
            data[i][2]= v.get(2).toString();
            data[i][3]=  v.get(3).toString();
            data[i][4]=  v.get(5).toString();           
            i++;
        }
        DefaultTableModel dtm = new DefaultTableModel(data,columns);
        jTable1.setModel(dtm);
    }
    
