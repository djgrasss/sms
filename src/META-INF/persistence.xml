<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="BankingAppPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>bankingapp.UserMds</class>
    <class>bankingapp.HeadTypesHasLedgerHeads</class>
    <class>bankingapp.FdDetails</class>
    <class>bankingapp.Transactions</class>
    <class>bankingapp.FdDurations</class>
    <class>bankingapp.MdsPayments</class>
    <class>bankingapp.Mds</class>
    <class>bankingapp.Settings</class>
    <class>bankingapp.MdsDetails</class>
    <class>bankingapp.TransactionDetails</class>
    <class>bankingapp.Members</class>
    <class>bankingapp.Accounts</class>
    <class>bankingapp.LedgerHeads</class>
    <class>bankingapp.MemberDetails1</class>
    <class>bankingapp.HeadTypes</class>
    <class>bankingapp.Loans</class>
    <class>bankingapp.Schemes</class>
    <properties>
      <property name="toplink.jdbc.user" value="root"/>
      <property name="toplink.jdbc.password" value=""/>
      <property name="toplink.jdbc.url" value="jdbc:mysql://localhost:3306/banking"/>
      <property name="toplink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
